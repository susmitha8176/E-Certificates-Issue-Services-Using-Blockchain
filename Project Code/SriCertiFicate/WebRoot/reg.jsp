        <div class="container">
    <div class="title"></div>
    <div class="content">
      <form action="regaction.jsp" method="get">
        <div class="user-details">
          <div class="input-box">
            <span class="details">Roll no</span>
            <input type="text" minlength = "4" maxlength="10"placeholder="Enter Roll Number" name="rollno" autofocus required="">
          </div>
          <div class="input-box">
            <span class="details">Name</span>
            <input type="text"minlength = "3" maxlength="17" placeholder="Enter your name" name="name"   required>
          </div>
          <div class="input-box">
            <span class="details">Password</span>
            <input type="password"pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{8,}" title="Must contain at least one number and one uppercase and lowercase letter, and at least 8 or more characters" placeholder="Enter your password" name="pass" required>
          </div>
          <div class="input-box">
            <span class="details">Email Id</span>
            <input type="email" placeholder="Enter your email" name="email" required="" title="please register with college mail id " pattern =".+@bvrithyderabad.edu.in">
          </div>
          <div class="input-box">
            <span class="details">course</span>
            <input type="text" placeholder="ex:cse" name="course" required>
          </div>
          <div class="input-box">
            <span class="details">Location</span>
            <input type="text" placeholder="location" name="loc" required>
          </div>
          <div class="input-box">
            <span class="details">contact No</span>
            <input type="tel" placeholder="mobile number" name="cno"  pattern="[0-9]{10}" required>
          </div>
          <div class="input-box">
            <span class="details">cgpa</span>
            <input type="text" placeholder="cgpa" name="cgpa"required>
          </div>
          <div class="input-box">
            <span class="details">Home City</span>
            <input type="text" minlength = "3"placeholder="city" name="sques1" required>
          </div>
          <div class="input-box">
            <span class="details">Nick Name</span>
            <input type="text" placeholder="nick" name="sques2" required>
          </div>
        </div>
